<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
<skipIfNotSupported/>
<defaultCurrentUser/>
<programGroup defaultName="Clipboard Boss"
            location="applications"/>
<shortcut
name="Clipboard Boss Test"
target="javaw.exe"
commandLine="-Djava.security.policy=.java.policy -jar $INSTALL_PATH\ClipboardBoss.jar"
description="Launch Clipboard Boss"
workingDirectory="$INSTALL_PATH"
iconFile="$INSTALL_PATH\ClipboardBoss.ico"
  initialState="normal"
programGroup="yes"
desktop="no"
applications="no"
startMenu="no"
startup="yes">

<createForPack name="Clipboard Boss Core"/>
</shortcut>

   <shortcut
     name="Clipboard Boss Uninstaller"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
     workingDirectory="$INSTALL_PATH\Uninstaller"
     iconFile="%SystemRoot%\system32\SHELL32.dll"
     iconIndex="31"
     description="Uninstall Clipboard Boss">

<createForPack name="Clipboard Boss Core"/>
   </shortcut>
   
</shortcuts>